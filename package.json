{
    "name": "www_liztoff_com",
    "version": "1.0.0",
    "description": "Maryland-area copywriter with a passion for creative writing. Her ever-fresh approach to writing is fortified by a dedication to improving interpersonal collaboration and team resources.",
    "scripts": {
        "build-apis": "webpack --config config/webpack.api.js",
        "build-dev": "webpack --config config/webpack.config.js",
        "build-prod": "webpack --config config/webpack.prod.js",
        "start": "webpack serve --config config/webpack.config.js",
        "start-prod": "webpack serve --config config/webpack.prod.js",
        "validate": "npm run check && npm run test",
        "prettier": "prettier --config config/.prettierrc.json --ignore-path config/.prettierignore --check '**'",
        "prettier-fix": "prettier --config config/.prettierrc.json --ignore-path config/.prettierignore --write '**'",
        "eslint": "eslint --config config/.eslintrc.json --ext '.ts,.tsx' src",
        "eslint-fix": "eslint --config config/.eslintrc.json --fix --ext '.ts,.tsx' src",
        "stylelint": "stylelint --config config/.stylelintrc.json src/**/*.scss",
        "stylelint-fix": "stylelint --config config/.stylelintrc.json src/**/*.scss --fix",
        "check": "npm run prettier && npm run eslint && npm run stylelint",
        "check-fix": "npm run prettier-fix && npm run eslint-fix && npm run stylelint-fix",
        "test": "jest --config config/jest.config.js",
        "release": "npm run build-prod && npm run validate && npm run build-apis",
        "deploy": "npm run release && ./scripts/deploy"
    },
    "author": "Dustin Toff",
    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.20.2",
        "@types/aws-lambda": "^8.10.109",
        "@types/aws-sdk": "^2.7.0",
        "@types/ejs": "^3.1.1",
        "@types/jest": "^27.4.1",
        "@types/node": "^18.11.9",
        "@types/xml2js": "^0.4.11",
        "@typescript-eslint/eslint-plugin": "^5.44.0",
        "@typescript-eslint/parser": "^5.44.0",
        "@vue/preload-webpack-plugin": "^2.0.0",
        "axios": "^1.2.0",
        "babel-loader": "^9.1.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.2",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "ejs": "^3.1.8",
        "ejs-compiled-loader": "^3.1.0",
        "eslint": "^8.28.0",
        "favicons": "^6.2.2",
        "favicons-webpack-plugin": "^5.0.2",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^27.5.1",
        "mini-css-extract-plugin": "^2.7.0",
        "postcss": "^8.4.19",
        "postcss-loader": "^7.0.1",
        "postcss-preset-env": "^7.8.3",
        "prettier": "^2.8.0",
        "s3-upload-custom-resource-lambda-layer": "^1.0.3",
        "sass": "^1.56.1",
        "sass-loader": "^13.2.0",
        "source-map-loader": "^4.0.1",
        "style-loader": "^3.3.1",
        "stylelint": "^14.15.0",
        "stylelint-config-idiomatic-order": "^9.0.0",
        "stylelint-config-standard-scss": "^6.1.0",
        "stylelint-scss": "^4.3.0",
        "ts-jest": "^27.1.4",
        "ts-loader": "^9.4.1",
        "typescript": "^4.9.3",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.0",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0",
        "xml2js": "^0.4.23"
    }
}
